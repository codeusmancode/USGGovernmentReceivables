  <SQLQuery><![CDATA[SELECT /*+ FIRST_ROWS_7 */         
       XxReabateRefund.REBATE_REFUND_ID,  
       XxReabateRefund.INVOICE_NUMBER,         
       XxReabateRefund.SHIPPING_PORT,         
       XxReabateRefund.SHIPPING_BILL_NO,         
       XxReabateRefund.SHIPPING_BILL_DATE,         
       XxReabateRefund.REBATE_AMOUNT,         
       XxReabateRefund.CASE_FILED_DATE,         
       case         
       when to_char(XxReabateRefund.CASE_FILED_DATE,'mm')<=6 then        
        to_char(add_months(XxReabateRefund.CASE_FILED_DATE,-12),'yyyy')||'-'||to_char(XxReabateRefund.CASE_FILED_DATE,'yyyy') else        
        to_char(XxReabateRefund.CASE_FILED_DATE,'yyyy')||'-'||to_char(add_months(XxReabateRefund.CASE_FILED_DATE,12),'yyyy')        
        end CLAIM_FILED_FINANCIAL_YEAR,        
       XxReabateRefund.REBATE_AMOUNT_RECEIVED,         
       XxReabateRefund.LESS_EXCESS,         
       XxReabateRefund.CHEQUE_NO,         
       XxReabateRefund.CHEQUE_DATE,         
       XxReabateRefund.CLAIM_REC_FINANCIAL_YEAR,         
       XxReabateRefund.STATUS,         
       XxReabateRefund.PENDING_AMOUNT,         
       XxReabateRefund.CREATED_BY,         
       XxReabateRefund.CREATION_DATE,         
       XxReabateRefund.UPDATED_BY,         
       XxReabateRefund.UPDATION_DATE,         
       XxReabateRefund.VERSION_NUMBER,         
       XxReabateRefund.REBATE_ESITMATE,         
       XxReabateRefund.REALIZATION_DATE,         
       XxReabateRefund.INVOICE_VALUE ,       
       XxReabateRefund.org_id,      
       XxReabateRefund.CONSULTANT_DISPATCH_DATE ,     
       hou.name||'('||hou.short_code||')' unit ,   
       XxReabateRefund.CLAIM_AGAINST ,  
       XxReabateRefund.COMMERCIAL_INVOICE_NUMBER
FROM  XX_REABATE_REFUND XxReabateRefund,      
hr_operating_units hou      
where hou.organization_id(+) = XxReabateRefund.org_id]]></SQLQuery>
